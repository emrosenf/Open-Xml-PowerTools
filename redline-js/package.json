{
  "name": "@docredline/core",
  "version": "0.1.0",
  "description": "Document comparison library for Word, Excel, and PowerPoint - TypeScript port of Open-Xml-PowerTools comparers",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun run tsc",
    "test": "bun run vitest run",
    "test:watch": "bun run vitest",
    "test:coverage": "bun run vitest run --coverage",
    "generate-golden": "cd .. && ./generate-golden.sh",
    "lint": "bun run eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "docx",
    "xlsx",
    "pptx",
    "document-comparison",
    "redline",
    "diff",
    "word",
    "excel",
    "powerpoint",
    "openxml"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fast-xml-parser": "^4.5.0",
    "js-sha256": "^0.11.0",
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
